<?php


//5. Calcule o tempo de uma viagem de carro. Pergunte a distância a percorrer e a velocidade média esperada
//para a viagem.


print "informe \n";

print "A distância a percorrer : ";
$distancia = (float) fgets(STDIN);

print "Velocidade média esperada:";
$velocidade = (float) fgets(STDIN);

$tempo = $distancia/$velocidade;

  
  print "O tempo de viagem será de $tempo horas";

